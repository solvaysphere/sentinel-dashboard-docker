#sentinel dev env
# 端口，默认8080
SERVER_PORT=8080
CONTEXT_PATH=/
# 控制台应用的 cookie 名称，可单独设置避免同一域名下 cookie 名冲突
SENTINEL_DASHBOARD_COOKIE_NAME=sentinel_dashboard_cookie
# 控制台应用的 session 的过期时间
SENTINEL_DASHBOARD_SESSION_TIMEOUT=7200
# 登录控制台的用户名
# 默认为 sentinel
SENTINEL_AUTH_USERNAME=sentinel
# 登录控制台的密码
# 默认为 sentinel
SENTINEL_AUTH_PASSWORD=sentinel
# 访问服务端连接
SENTINEL_DASHBOARD_SERVER=127.0.0.1:8080
# 项目名称
SENTINEL_DASHBOARD_PROJECT_NAME=sentinel-dashboard
# 是否开启登录鉴权，仅用于日常测试，生产上不建议关闭
AUTH_ENABLED=true
# 是否隐藏无健康节点的应用，距离最近一次主机心跳时间的毫秒数，默认关闭
# 最小值: 60000
SENTINEL_DASHBOARD_APP_HIDEAPPNOMACHINEMILLIS=0
# 是否自动删除无健康节点的应用，距离最近一次其下节点的心跳时间毫秒数，默认关闭
# 最小值: 120000
SENTINEL_DASHBOARD_REMOVEAPPNOMACHINEMILLIS=0
# 主机失联判定，不可关闭
# 最小值: 30000
SENTINEL_DASHBOARD_UNHEALTHYMACHINEMILLIS=60000
# 距离最近心跳时间超过指定时间是否自动删除失联节点，默认关闭
# 最小值: 300000
SENTINEL_DASHBOARD_AUTOREMOVEMACHINEMILLIS=0

